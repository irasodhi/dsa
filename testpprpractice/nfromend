Node* findlast(Node* head, int n) {
    if (head == NULL || n <= 0) return head;
    Node* fast = head;
    Node* slow = head;
 
    int count = 0;
    while (count < n && fast != NULL) {
        fast = fast->next;
        count++;
    }

    
    if (fast == NULL) {
        while (slow->next != NULL) slow = slow->next;
        return slow;   
    }
 
    while (fast != NULL) {
        fast = fast->next;
        slow = slow->next;
    }

    return slow;
}